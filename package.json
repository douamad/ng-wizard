{
  "name": "ng-wizard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4201",
    "build": "ng build --aot",
    "lib:build": "rimraf dist && ng build ng-wizard --prod",
    "lib:themes": "mkdir dist\\ng-wizard\\themes && copy src\\ng-wizard\\themes\\*.css dist\\ng-wizard\\themes",
    "lib:files": "copy README.md dist\\ng-wizard && copy LICENSE dist\\ng-wizard",
    "lib:pack": "cd dist/ng-wizard && npm pack",
    "lib": "npm run lib:build && npm run lib:themes && npm run lib:files && npm run lib:pack",
    "test": "ng test ng-wizard",
    "lint": "ng lint ng-wizard",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "bootstrap": "^4.3.1",
    "core-js": "^3.21.0",
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.11",
    "@angular/cli": "^13.3.9",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "^13.3.11",
    "@types/jasmine": "^3.6.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.33",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.1",
    "protractor": "^7.0.0",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}
